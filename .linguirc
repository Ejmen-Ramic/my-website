{
  "catalogs": [
    {
      "path": "<rootDir>/src/locales/{locale}/messages",
      "include": [
        "<rootDir>/src"
      ]
    }
  ],
  "locales": [
    "en",
    "ba"
  ],
  "sourceLocale": "en",
  "format": "po",
  "extractors": [
    "babel"
  ],
  "orderBy": "messageId",
  "rootDir": "."
}
